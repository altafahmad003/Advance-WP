<?php

$i = 6;

switch ($i){  
    case "1":  
        echo "The number is One"; 
        break;            
    case "2":
        echo "The number is Two";
        break;
    case "3":
        echo "The number is Three";
        break;
    case "4":
        echo "The number is Four";
        break;
    case "5":
        echo "The number is Five";
        break;
    default:    
    echo "Your number is greater than 5.";
    

}
?>
